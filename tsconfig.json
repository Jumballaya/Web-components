{
  "compilerOptions": {
    "outDir": "./dist/",
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "lib": [
      "es2016",
      "dom",
      "dom.iterable"
    ],
    "module": "commonjs",
    "moduleResolution": "Node",
    "sourceMap": true,
    "target": "es6",
    "types": [
      "reflect-metadata"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}